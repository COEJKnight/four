{
    "extends": "airbnb",
    "rules": {
        // Override any settings from the airbnb configuration
        // "indent": [2, 4],
        "comma-dangle": 0,
        // To fix
        "brace-style": 0,
        "camelcase": 0,
        "comma-spacing": 0,
        "dot-notation": 0,
        "eol-last": 0,
        "eqeqeq": 0,    // ASAP
        "guard-for-in": 0,
        "indent": 0,
        "key-spacing": 0,
        "max-len": 0,
        "no-console": 0,
        "no-mixed-spaces-and-tabs": 0,
        "no-multiple-empty-lines": 0,
        "no-multi-spaces": 0,
        "no-param-reassign": 0,
        "no-trailing-spaces": 0,
        "no-unused-vars": 0,
        "no-use-before-define": 0,  // ASAP
        "object-curly-spacing": 0,
        "object-shorthand": 0,
        "padded-blocks": 0,
        "prefer-const": 0,
        "quote-props": 0,
        "quotes": 0,
        "semi": 0,
        "space-infix-ops": 0,
    }
}
